<% if (messages.length) { %>
<div class="alert alert-dismissible <%= messageClass %>">
    <button type="button" class="close" data-dismiss="alert">Ã—</button>
    <% if (messages.length === 1) { %>
    <p><%= messages[0] %></p>
    <% } else { %>
    <ul>
        <% messages.forEach(function (message) { %>
        <li><%= message %></li>
        <% }) %>
    </ul>
    <% } %>
</div>
<% } %>
